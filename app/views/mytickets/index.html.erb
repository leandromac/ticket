<%- model_class = Myticket -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:id_customer) %></th>
      <th><%= model_class.human_attribute_name(:subject) %></th>
      <th><%= model_class.human_attribute_name(:departament) %></th>
      <th><%= model_class.human_attribute_name(:category) %></th>
      <th><%= model_class.human_attribute_name(:mensage) %></th>
      <th><%= model_class.human_attribute_name(:upload) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:deadline) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @mytickets.each do |myticket| %>
      <tr>
        <td><%= link_to myticket.id, myticket_path(myticket) %></td>
        <td><%= myticket.id_customer %></td>
        <td><%= myticket.subject %></td>
        <td><%= myticket.departament %></td>
        <td><%= myticket.category %></td>
        <td><%= myticket.mensage %></td>
        <td><%= link_to 'Baixar anexo', myticket.upload_url %></td>
        <td><%= myticket.status %></td>
        <td><%= myticket.date %></td>
        <td><%= myticket.deadline %></td>
        <td><%=l myticket.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_myticket_path(myticket), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      myticket_path(myticket),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_myticket_path,
            :class => 'btn btn-primary' %>
